/**app.wxss**/
page{
    font-size: 30rpx;
    color: #333;
}
.container {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

page {
    background: #f5f5f5;
}

.all-bg-img {
    width: 100%;
    height: 252rpx;
    position: fixed;
    top: 0;
    left: 0;
}

.all-bg-img image {
    width: 100%;
    height: 100%;
}

.not_data{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #fff;
    z-index: 99;
}
.not_data .img{
    margin-top: 353rpx;
    width: 100%;
    text-align: center;
}
.not_data .img image{
    width: 344rpx;
    height: 275rpx;
}
.not_data .contxt{
    text-align: center;
    margin-top: 56rpx;
    color: #333;
}
